    "    while True:\n",
    "        weight = float(input(\"What's your weight in kg?: \"))\n",
    "        height = float(input(\"What's your height in meters?: \"))\n",
    "        break\n",
    "    print(f'Your BMI is: {BMI(weight, height)}')\n",
    "        \n",
    "def BMI(kg, m):\n",
    "    BMI = kg/(m**2)\n",
    "    return BMI\n",
